<% provide(:title, @group.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="group_info">
      <h1>
        <%= @group.name %>
      </h1>
    </section>
    <section class="stats">
      <%= render 'shared/stats_user' %>
    </section>
  </aside>
  <div class="col-md-8">
    <%= render 'join_form' %>
    <h1>Members</h1>
    <% @users.each do |user| %>
      <%= link_to user.name, user %>
    <% end %>
  </div>
